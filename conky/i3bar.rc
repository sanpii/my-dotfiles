out_to_x no
background no
out_to_console yes
update_interval 1.0
total_run_times 0
use_spacer none
short_units yes
if_up_strictness address
use_spacer left

TEXT
${image ~/.conky/icons/cpu.xbm} ${cpu cpu0}% \
${image ~/.conky/icons/cpu.xbm} ${cpu cpu1}% \
${image ~/.conky/icons/mem.xbm} $memperc% \
${image ~/.conky/icons/clock.xbm} ${time %x %X}
